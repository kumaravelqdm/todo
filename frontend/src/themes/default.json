{
    "direction": "ltr",
    "typography": {
      "fontFamily": "regular"
    },
    "components": {
      "MuiButton": {
        "styleOverrides": {
          "root": {
            "textTransform": "capitalize",
            "fontWeight": "medium"
          }
        }
      },
      "MuiOutlinedInput": {
        "styleOverrides": {
          "root": {
            "borderRadius": "4px",
            "marginTop": 4
          }
        }
      }
    },
    "palette": {
      "mode": "light",
      "primary": {
