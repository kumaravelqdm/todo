        "borderInput": "rgba(0, 0, 0, 0.23)",
        "rejected": "#FADCDC",
        "approved": "#DCFAE8",
        "lightGreen": "#FAFAFA",
        "profileCard": "#F1F6FF",
        "highLight": "#EEF3FF",
        "statusCard": "#FBDDDD",
        "shadowColor": "#0000000D",
        "dateBg": "#E3ECFF"
