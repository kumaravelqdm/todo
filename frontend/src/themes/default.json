        "bgColor": "#EEF1FD"
      },
      "secondary": {
        "light": "#e6b136",
        "main": "#E09E04",
        "dark": "#9c6e02",
        "contrastText": "#fff"
      },
      "success": {
        "light": "#D4FFE5",
        "main": "#21C262",
        "dark": "#178744",
        "contrastText": "#fff"
      },
      "warning": {
        "light": "#ffac3e",
        "main": "#FF980E",
        "dark": "#b26a09",
        "contrastText": "#fff"
      },
      "error": {
        "light": "#FDE6E6",
        "main": "#FB4E4E",
        "dark": "#d32f2f",
        "contrastText": "#fff"
      },
      "text": {
        "primary": "#141414",
        "secondary": "#4D4D4D",
        "disabled": "#d32f2f",
        "tertiary": "#808080",
        "placeHolder": "#CACACA",
        "green": "#2ECA45",
        "grey": "#E4E8EE",
        "lightGrey": "#F2F2F2",
        "darkRed": "#F56666",
        "labelText": "#323232",
        "black":"#323232"
  
      },
      "background": {
        "primary": "#F9F9F9",
        "secondary": "#fff",
        "shadow": "#00000029",
